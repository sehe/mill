#!/usr/bin/env runmill
#(
A program which demonstrates how to define and call procedures.
)

use std::io;

proc greet(name: String) {
    io::writeln("Hello, " ~ name ~ "!");
}

proc ungreet(name: String) {
    io::writeln("Goodbye, " ~ name ~ "!");
}

MAIN {
    greet("rightfold");
    greet("world");
}
