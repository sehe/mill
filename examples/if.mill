use std::io;

MAIN {
    if true {
        io::writeln("OK");
    } else {
        io::writeln("ERROR");
    };

    if false {
        io::writeln("ERROR");
    } else {
        io::writeln("OK");
    };

    if true {
        io::writeln("OK");
    };

    if false {
        io::writeln("ERROR");
    };
}
